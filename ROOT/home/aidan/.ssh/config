# CF

Host sw10
    HostName sw10.sfo10
    ProxyJump 36netops9

Host sw9
  HostName sw09.sfo10
  ProxyJump 36netops9

Host stash.cfops.it bitbucket.cfdata.org
  HostName git.cfdata.org
  # on Apple M1 systems use /opt/homebrew/bin/cloudflared instead
  ProxyCommand /usr/local/bin/cloudflared access ssh --hostname %h
  
Host *.ssh.cfdata.org
  # on Apple M1 systems use /opt/homebrew/bin/cloudflared instead
  ProxyCommand /usr/local/bin/cloudflared access ssh --hostname %h

Host *dm* !*.cfops.net
  ProxyCommand cloudflared access ssh --hostname %h.ssh.cfdata.org
  StrictHostKeyChecking accept-new
    
Host 36*
  ProxyCommand ssh pdx01.ssh.cfdata.org -W %h:%p
Host 348*
  ProxyCommand ssh pdx05.ssh.cfdata.org -W %h:%p
Host 349*
  ProxyCommand ssh pdx06.ssh.cfdata.org -W %h:%p
Host 420*
  ProxyCommand ssh ams05.ssh.cfdata.org -W %h:%p
 
Host *
  # May be required for macOS ssh-agent (uncomment if necessary)
  #UseKeychain yes
  # this might only apply to macOS
  AddKeysToAgent yes
  User aidan